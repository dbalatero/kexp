				});
			// end debug

			$('a[data-favorite="true"]').click(function () {
				unlikePlay($(this));
				return false;
			});
			$('a[data-favorite="false"]').click(function () {
				likePlay($(this));
				return false;
			});
			
		}); // end onload functions

    
